<section class="TodoSection">
  <div class="TodoSectionTitle">
    <p class="TodoSectionTitle-text"><%= section_title %></p>
    <p class="TodoSectionTitle-remainingcount"><%= todos.completed.count %> /<%= todos.count %></p>
  </div>
  <ul>
  <% todos.each do |todo| %>
    <li class="TodoItem">

      <% if todo.completed %>
        <input class="TodoItem-checkbox" type="checkbox" checked>
        <p class="TodoItem-textlinethrough"><%= todo.todo_text %></p>
      <% else %>
        <input class="TodoItem-checkbox" type="checkbox" >
        <%= todo.todo_text %>
      <% end %>

      <% if show_due_date %>
        <p class="TodoItem-date"><%= todo.due_date.to_s(:short) %></p>
      <% end %>

      <a class="TodoItem-delete" "href"=" javascript:void(0)">
        <img src="images.png" width="12" height="12">
      </a>
    </li>

    <% end %>
  </ul>
</section>
